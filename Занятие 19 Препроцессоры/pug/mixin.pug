doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
        mixin menu
            nav
                ul 
                    li: a asdasdasd
                    li: a 12323
                    li: a 12323

        +menu
        +menu
        +menu
        +menu
        +menu

        

        // MENU2
        mixin menu2(className)
            nav
                ul(class=className)
                    li: a asdasdasd
                    li: a 12323
                    li: a 12323
        +menu2("a")
        +menu2("b")
        +menu2("c")
        +menu2("d")

        // CARD
        mixin card(title, text, price)
            .card
                .card-image
                .card-content
                    .card-content-title #{title}
                    .card-content-text #{text}
                    .card-content-price #{price} руб
        +card('Резиновые тапочки', '123123', 400)
        +card('Сланцы', 'Дщкуь шзыгь вщдщк', 500)
        +card('Резиновые тапочки', '123123', 400)
        +card('Резиновые тапочки', '123123', 400)
        +card('Резиновые тапочки', '123123', 400)
        +card('Резиновые тапочки', '123123', 400)
        +card('Резиновые тапочки', '123123', 400)
